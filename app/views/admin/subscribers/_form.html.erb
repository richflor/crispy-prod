<%= form_with model: [:admin, @subscriber] do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
    <% @subscriber.errors.full_messages_for(:email).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
